<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.byrj.pet.pet"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="com.byrj.pet.base.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.byrj.pet.fun.login.AccountActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.byrj.pet.fun.MainActivity"/>
        <activity android:name="com.byrj.pet.fun.login.password.PasswordActivity"/>
        <activity android:name="com.byrj.pet.fun.login.code.VerificationCodeActivity"/>
    </application>

</manifest>